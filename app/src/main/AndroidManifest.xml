<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.handshaker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/CustomActionBarTheme">

        <activity android:name=".CalificacionDatos"
            />

        <activity
            android:name=".inicioTrabajador"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".DatosTrabajadores"
            android:label="Ingresar Información" />

        <activity
            android:name=".DatosClientes"
            android:label="Ingresar Información" />

        <activity
            android:name=".inicioSesion"
            android:label="Iniciar Sesión"/>

        <activity
            android:name=".Registro"
            android:label="Registro" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>